{
  "functions": {
    "app/api/webhook/cardcom/route.ts": {
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/api/webhook/cardcom",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "POST, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, x-cardcom-signature"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/webhook/cardcom",
      "destination": "/api/webhook/cardcom"
    }
  ]
}
